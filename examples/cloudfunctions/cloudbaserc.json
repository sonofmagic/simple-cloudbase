{
  "version": "2.0",
  "envId": "{{env.ENV_ID}}",
  "functionRoot": "dist",
  "functions": [
    {
      "name": "getOpenId",
      "installDependency": true,
      "runtime": "Nodejs12.16",
      "ignore": [
        "*.md",
        "node_modules",
        "node_modules/**/*",
        "**/*.map"
      ],
